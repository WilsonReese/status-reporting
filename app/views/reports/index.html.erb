<p id="notice"><%= notice %></p>

<h1>Reports</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Period</th>
      <th>Current committees rocks</th>
      <th>Future committees rocks</th>
      <th>Current internal support</th>
      <th>Future internal support</th>
      <th>Current projects</th>
      <th>Future projects</th>
      <th>Current other</th>
      <th>Future other</th>
      <th>Pto</th>
      <th>Pvt</th>
      <th>Ooo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= report.user_id %></td>
        <td><%= report.period_id %></td>
        <td><%= report.current_committees_rocks %></td>
        <td><%= report.future_committees_rocks %></td>
        <td><%= report.current_internal_support %></td>
        <td><%= report.future_internal_support %></td>
        <td><%= report.current_projects %></td>
        <td><%= report.future_projects %></td>
        <td><%= report.current_other %></td>
        <td><%= report.future_other %></td>
        <td><%= report.pto %></td>
        <td><%= report.pvt %></td>
        <td><%= report.ooo %></td>
        <td><%= link_to 'Show', report %></td>
        <td><%= link_to 'Edit', edit_report_path(report) %></td>
        <td><%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Report', new_report_path %>
